{
    "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/plugin-schema.json",
    "creator": "jopemachine",
    "name": "arvis-naver-search-plugin",
    "enabled": true,
    "description": "Naver search plugin for Arvis",
    "readme": "",
    "version": "0.0.4",
    "webAddress": "https://github.com/jopemachine/arvis-naver-search-plugin",
    "main": "index.js",
    "defaultIcon": "icon.png",
    "actions": [
        {
            "type": "open",
            "modifiers": "normal",
            "target": "http://search.naver.com/search.naver?ie=utf8&sm=stp_hty&where=se&query={query}"
        }
    ]
}